<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        android:id="@+id/ab"
        motion:constraintSetEnd="@+id/b"
        motion:constraintSetStart="@+id/a"
        motion:pathMotionArc="startHorizontal"
        motion:motionInterpolator="easeIn"
        motion:duration="1000">

        <OnClick motion:targetId="@+id/button30" />
    </Transition>
    <Transition
        motion:constraintSetEnd="@+id/c"
        motion:constraintSetStart="@+id/b"
        motion:pathMotionArc="startVertical"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="linear"
        motion:duration="1000"/>
    <Transition
        android:id="@+id/cd"
        motion:constraintSetEnd="@+id/d"
        motion:constraintSetStart="@+id/c"
        motion:pathMotionArc="startHorizontal"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="linear"
        motion:duration="1000"/>
    <Transition
        android:id="@+id/da2"
        motion:constraintSetEnd="@+id/a2"
        motion:constraintSetStart="@+id/d"
        motion:pathMotionArc="startVertical"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="linear"
        motion:duration="1000"/>
    <Transition
        motion:constraintSetEnd="@+id/e"
        motion:constraintSetStart="@+id/a2"
        motion:pathMotionArc="startHorizontal"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="linear"
        motion:duration="1000"/>
    <Transition
        android:id="@+id/ef"
        motion:constraintSetEnd="@+id/f"
        motion:constraintSetStart="@+id/e"
        motion:pathMotionArc="startVertical"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="linear"
        motion:duration="1000"/>
    <Transition
        motion:constraintSetEnd="@+id/g"
        motion:constraintSetStart="@+id/f"
        motion:pathMotionArc="startHorizontal"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="linear"
        motion:duration="1000"/>
    <Transition
        motion:constraintSetEnd="@+id/a"
        motion:constraintSetStart="@+id/g"
        motion:pathMotionArc="startVertical"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="easeOut"
        motion:duration="1000"/>
    <ConstraintSet android:id="@+id/a">
        <Constraint
            android:id="@+id/button"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintVertical_bias="0.5"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/b">
        <Constraint
            android:id="@+id/button"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintVertical_bias="0.25"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/c">
        <Constraint
            android:id="@+id/button"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/d">
        <Constraint
            android:id="@+id/button"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintVertical_bias="0.25"
            motion:layout_constraintHorizontal_bias="1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/a2" motion:deriveConstraintsFrom="@+id/a"/>

    <ConstraintSet android:id="@+id/e">
        <Constraint
            android:id="@+id/button"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintVertical_bias="0.75"
            motion:layout_constraintHorizontal_bias="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/f">
        <Constraint
            android:id="@+id/button"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintVertical_bias="1"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/g">
        <Constraint
            android:id="@+id/button"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintVertical_bias="0.75"
            motion:layout_constraintHorizontal_bias="1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
</MotionScene>